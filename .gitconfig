[user]
  email = n.ishida0520@gmail.com
  name = Naoki Ishida
[color]
  ui = auto
  status = auto
  diff = auto
  branch = auto
  interactive = auto
  grep = auto
[color "diff"]
  plain = normal
  meta = bold green
  frag = bold cyan
  func = bold magenta
  old = bold red
  new = bold blue
  commit = bold white
  whitespace = normal magenta
[alias]
  co          = checkout
  ci          = commit
  br          = branch
  bra         = branch -a
  st          = status
  d           = diff
  last        = log -1 HEAD
  unstage     = reset HEAD --
  glog        = log --graph --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
  log-all     = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'

